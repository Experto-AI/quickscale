# Test Environment Variables for PostgreSQL Testing
# These variables configure the PostgreSQL test database

# Test Database Configuration
TEST_DB_NAME=quickscale_test
TEST_DB_USER=test_user
TEST_DB_PASSWORD=test_pass
TEST_DB_HOST=localhost
TEST_DB_PORT=5433

# Test-specific optimizations
TEST_DB_CONN_MAX_AGE=0
TEST_DB_ATOMIC_REQUESTS=True

# Disable migrations for faster tests
DJANGO_TEST_MIGRATIONS=False
