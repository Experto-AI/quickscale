{%raw%}{% load static %}<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <link rel="icon" type="image/svg+xml" href="{% static 'frontend/favicon.svg' %}" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{%endraw%}{{ project_name }}{%raw%}</title>
    <meta name="description" content="{%endraw%}{{ project_name }}{%raw%} - Built with QuickScale and React" />
    <link rel="stylesheet" href="{% static 'frontend/assets/index.css' %}" />
  </head>
  <body>
    <div id="root"></div>
    <script>
      window.__QUICKSCALE__ = {
        projectName: "{%endraw%}{{ project_name }}{%raw%}",
        modules: {
          auth: {% if 'quickscale_modules_auth' in settings.INSTALLED_APPS %}true{% else %}false{% endif %},
          blog: {% if 'quickscale_modules_blog' in settings.INSTALLED_APPS %}true{% else %}false{% endif %},
          listings: {% if 'quickscale_modules_listings' in settings.INSTALLED_APPS %}true{% else %}false{% endif %},
          crm: {% if 'quickscale_modules_crm' in settings.INSTALLED_APPS %}true{% else %}false{% endif %},
          billing: {% if 'quickscale_modules_billing' in settings.INSTALLED_APPS %}true{% else %}false{% endif %},
          teams: {% if 'quickscale_modules_teams' in settings.INSTALLED_APPS %}true{% else %}false{% endif %}
        }
      };
    </script>{%endraw%}
    <script type="module" src="{%raw%}{% static 'frontend/assets/index.js' %}{%endraw%}"></script>
  </body>
</html>
